{
  "name": "lingo-challenge",
  "version": "0.0.1",
  "description": "A Quasar Project",
  "productName": "LingoChallenge",
  "author": "",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{js,cjs,mjs,vue}\"",
    "format": "prettier --write \"**/*.{js,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar dev",
    "build": "quasar build",
    "postinstall": "quasar prepare",
    "backend:install": "cd backend && bun install",
    "backend:install:npm": "cd backend && npm install",
    "backend:setup": "cd backend && bun run db:migrate && bun run db:seed",
    "backend:setup:npm": "cd backend && npm run db:migrate && npm run db:seed",
    "backend:dev": "cd backend && bun src/auth-server.js",
    "backend:dev:npm": "cd backend && npm run dev:node",
    "setup": "bun run backend:install && bun run backend:setup",
    "dev:full": "concurrently \"bun run backend:dev\" \"bun run dev\"",
    "dev:npm": "concurrently \"npm run backend:dev:npm\" \"npm run dev\"",
    "start:bun": "bun run start-bun.js"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.4",
    "quasar": "^2.16.0",
    "vue": "^3.4.18",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@quasar/app-vite": "^2.1.0",
    "@vue/eslint-config-prettier": "^10.1.0",
    "autoprefixer": "^10.4.2",
    "concurrently": "^9.2.0",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^9.30.0",
    "globals": "^15.12.0",
    "postcss": "^8.4.14",
    "prettier": "^3.3.3",
    "vite-plugin-checker": "^0.9.0"
  },
  "engines": {
    "node": "^28 || ^26 || ^24 || ^22 || ^20 || ^18",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
